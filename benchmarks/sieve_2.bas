010 REM***A PRIME NUMBER SIEVE BENCHMARK from 8bitworkshop
022 C = 0
025 N = 1024
030 DIM F(N+1)
040 FOR I = 0 TO N : F(I)=1 : NEXT I
050 S = N
060 FOR I = 0 TO S
070   IF F(I)=0 THEN GOTO 110
080   P = I+I+3
085   IF I+P>S THEN GOTO 110
090   FOR K=I+P TO S STEP P : F(K)=0 : NEXT K
100   C = C+1
110 NEXT I
130 PRINT C;" PRIMES FOUND "
150 END
