100 DATA 1.5,2.7,3.14,"HELLO"
110 DATA 0,1,-2,"WORLD"
120 READ A,B,C
130 IF A<>1.5 THEN STOP
140 IF B<>2.7 THEN STOP
150 IF C<>3.14 THEN STOP
160 RESTORE
170 REM This next will throw an error, if the restore didn
180 READ A,B,C
190 READ D$
200 IF D$<>"HELLO" THEN STOP
210 RESTORE 110
220 READ A,B,C,D$
230 IF A<>0 THEN STOP
240 IF B<>1 THEN STOP
250 IF C<>-2 THEN STOP
260 IF D$<>"WORLD" THEN STOP
270 END
